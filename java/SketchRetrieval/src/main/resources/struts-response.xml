<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="sketch" namespace="/sketch" extends="struts-default">

		<action name="req" class="com.hust.response.impl.Response"
			method="excute">
			<result name="success">/WEB-INF/page/response.jsp</result>
		</action>
		<action name="img" class="com.hust.response.impl.ImageView"
			method="excute">
			<result name="success">/WEB-INF/page/imageview.jsp</result>
		</action>
	</package>
	
</struts>